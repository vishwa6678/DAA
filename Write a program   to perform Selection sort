
#include <stdio.h>
int main() {
    int n, i, j, temp;
    int arr[]={9,8,5,2,0,1};
    n=sizeof(arr)/sizeof(arr[0]);
    for (i = 0; i < n - 1; i++) {
        for (j = i + 1; j < n; j++) {
            if (arr[i] > arr[j]) {
                temp = arr[i];
                arr[i] = arr[j];
                arr[j] = temp;
            }
        }
    }
    printf("Sorted array in ascending order:\n");
    for (i = 0; i < n; i++) {
        printf(",%d",arr[i]);
    }
    return 0;
}
